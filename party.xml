<tryton>
    <data>
        <record model="ir.ui.menu-res.group" id="menu_category_list_group_party_admin">
            <field name="menu" ref="party.menu_category_list"/>
            <field name="group" ref="party.group_party_admin"/>
        </record>

        <record model="ir.property" id="property_party_lang">
            <field name="field"
                search="[('model.model', '=', 'party.party'), ('name', '=', 'lang')]"/>
            <field name="value" eval="'ir.lang,' + str(ref('ir.lang_es'))"/>
        </record>

        <menuitem
            id="menu_act_searching_party"
            parent="party.menu_party"
            action="searching.act_searching"
            icon="tryton-find"/>
    </data>
</tryton>
